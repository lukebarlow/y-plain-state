{
  "name": "y-plain-state",
  "description": "Synchronise javascript data structures across clients",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukebarlow/y-plain-state.git"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "test": "jasmine ./src/specs/",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "npm run build:commonjs && cross-env BABEL_ENV=commonjs NODE_ENV=development webpack lib/index.js dist/y-plain-state.js",
    "build": "npm run build:umd",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.0.0",
    "jasmine-es6": "^0.1.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5",
    "y-test": "^0.5.0",
    "y-websockets-client": "^8.0.16"
  },
  "dependencies": {
    "d3-dispatch": "^1.0.1",
    "y-array": "^10.0.6",
    "y-map": "^10.0.5",
    "yjs": "^12.1.7"
  },
  "peerDependencies": {}
}
